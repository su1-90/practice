<%= stylesheet_link_tag 'style', 'new', medis: 'all', 'data-turbolinks-track': 'reload' %>

<div class="new_continer">
  <h1>New User</h1>
  <%= form_with(model: @user) do |form| %>
    <ul>
      <li>
        <%= form.label :name %>
        <%= form.text_field :name %>
      </li>

      <li>
        <%= form.label :email %>
        <%= form.text_field :email %>
      </li>

      <li>
        <%= form.label :password %>
        <%= form.text_field :password %>
      </li>
    </ul>

      <li>
        <%= form.submit %>
      </li>
  <% end %>

    <%= link_to "Back to users", users_path %>
</div>